{
  "manifest_version": 2,
  "name": "Canvas P5",
  "version": "1.0",
  "description": "Injects a p5.js sketch",
  "icons": {
    "48": "p5js.svg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["p5.min.js", "sketch.js"]
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "Error1.png",
    "Error2.png",
    "Error3.png",
    "Error4.png",
    "Error5.png"
  ],
  "applications": {
    "gecko": {
      "id": "you@example.com"
    }
  }
}
