{
  "manifest_version": 2,
  "name": "ErrorMessage",
  "version": "1.0",
  "description": "Displays a fullscreen interactive p5.js canvas",
  "icons": {
    "48": "p5js.svg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["p5.min.js", "sketch.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": ["<all_urls>"],
  "web_accessible_resources": [
    "images/Error1.png",
    "images/Error2.png",
    "images/Error3.png",
    "images/Error4.png",
    "images/Error5.png"
  ],
  "applications": {
    "gecko": {
      "id": "laura.camargo06@gmail.com"
    }
  }
}
